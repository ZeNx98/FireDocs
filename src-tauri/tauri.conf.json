{
    "build": {
        "beforeDevCommand": "",
        "beforeBuildCommand": "",
        "devPath": "../web",
        "distDir": "../web",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "firedoc",
        "version": "3.2.0"
    },
    "tauri": {
        "allowlist": {
            "all": true,
            "fs": {
                "all": true,
                "scope": [
                    "$HOME/**",
                    "$APPDATA/**"
                ]
            },
            "protocol": {
                "all": true,
                "asset": true,
                "assetScope": [
                    "$HOME/**",
                    "/"
                ]
            }
        },
        "bundle": {
            "active": true,
            "category": "Utility",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "rpm": {
                "depends": [
                    "webkit2gtk4.0",
                    "gtk3",
                    "libappindicator-gtk3"
                ]
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.png"
            ],
            "identifier": "com.znx.firedoc",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": "default-src 'self' 'unsafe-inline' 'unsafe-eval' tauri: asset: https: data: blob: pdfstream:;"
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 800,
                "resizable": true,
                "title": "FireDoc",
                "width": 1200,
                "url": "homepage.html"
            }
        ]
    }
}